<det:KLAMapping> a rr:TriplesMap;
  rml:logicalSource [
    rml:source "nesa.json";
    rml:referenceFormulation ql:JSONPath;
    rml:iterator "$"
  ];

  rr:subjectMap [
    rr:template "http://thesaurus.education.nsw.gov.au/kla/{id}";
    rr:class det:KLA
  ];

  rr:predicateObjectMap [
    rr:predicate dc:title;
      rr:objectMap [
        rml:reference "text"
      ]
  ];

  rr:predicateObjectMap [
    rr:predicate asn:altStatementNotation;
      rr:objectMap [
        rml:reference "asn_altStatementNotation.literal";
        rr:language "en-au"
      ]
  ];

  rr:predicateObjectMap [
    rr:predicate asn:authorityStatus;
      rr:objectMap [
        rml:reference "asn_authorityStatus.uri";
	    rr:termType rr:IRI
